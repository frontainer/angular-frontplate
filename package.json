{
  "name": "ng2-frontplate",
  "version": "0.0.1",
  "description": "angular2 starter kit with frontplate-cli",
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@angular/router": "^3.1.2",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.1.2",
    "@ngtools/webpack": "^1.1.4",
    "@types/node": "^6.0.46",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "raw-loader": "^0.5.1",
    "source-map-explorer": "^1.3.2",
    "typescript": "^2.0.6"
  },
  "scripts": {
    "start": "frp build && frp serve -t",
    "test": "frp task test",
    "build": "frp build",

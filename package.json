{
  "name": "ng2-frontplate",
  "version": "0.0.1",
  "description": "angular2 starter kit with frontplate-cli",
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@ngtools/webpack": "1.2.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.2",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.1",
    "@ngtools/webpack": "1.2.1",
    "@types/node": "6.0.55",
    "angular2-router-loader": "0.3.4",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "connect-history-api-fallback": "^1.3.0",
    "extract-loader": "0.1.0",
    "file-loader": "0.9.0",
    "postcss-loader": "1.2.1",
    "raw-loader": "0.5.1",
    "sass-loader": "4.1.1",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.13.1",
    "typescript": "2.0.10",
    "webpack-build-notifier": "0.1.13"
  },
  "scripts": {
    "start": "frp build && frp serve",
    "test": "frp test",
    "build": "npm run dll && frp build",
    "serve": "frp serve",
    "dll": "frp task script -p -c frp.dll.config.js",
    "production": "npm run dll && frp build -p",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -w -s -r 0",
    "map": "source-map-explorer public/assets/js/app.js public/assets/js/maps/app.map"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:frontainer/ng2-frontplate.git"
  },
  "author": "frontainer",
  "license": "MIT",
  "private": true
}
